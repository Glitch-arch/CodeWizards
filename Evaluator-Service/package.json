{
  "name": "evaluator-service",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn tsc",
    "lint": "eslint src/**/*.ts",
    "eslint:fix" : "eslint src/**/*.ts --fix",
    "watch": "yarn tsc -w",
